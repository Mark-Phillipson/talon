title: /Coloring Game 2/
-
zoom in: key("keypad_plus")
zoom out: key("minus")

place that:
    mouse_click(3)
    sleep(50ms)
    mouse_click(0)

paint left:
    key("left")
    sleep(200ms)
    mouse_click(0)

paint right:
    key("right")
    sleep(200ms)
    mouse_click(0)

paint up:
    key("up")
    sleep(200ms)
    mouse_click(0)

paint down:
    key("down")
    sleep(200ms)
    mouse_click(0)